mixin breadcrumb(list , className = "")
    nav.box-breadcrumb
        div(class = className)
            ul.box-breadcrumb__list
                each item in list 
                    li.box-breadcrumb__item 
                        a(href="#" class = "box-breadcrumb__link") #{item.title}
                        <i class="fa-solid fa-angle-right"></i>